{
  "name": "estudo-herbs",
  "version": "1.0.0",
  "description": "project generated by herbs-cli",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "start:dev": "nodemon src/index.js",
    "lint": "npx eslint \"**/*.{js,jsx}\" --quiet",
    "lint:fix": "npx eslint \"**/*.{js,jsx}\" --fix",
    "knex:make": "npx knex --knexfile knexFile.js migrate:make",
    "knex:migrate": "npx knex --knexfile knexFile.js migrate:latest",
    "knex:rollback": "npx knex --knexfile knexFile.js migrate:rollback",
    "knex:makeSeeds": "npx knex --knexfile knexFile.js seed:make",
    "knex:runSeeds": "npx knex --knexfile knexFile.js seed:run",
    "test": "mocha --timeout 999999 --colors --recursive src/**/*.test.js"
  },
  "keywords": [
    "herbs"
  ],
  "author": "samuel.carvalho",
  "license": "MIT",
  "dependencies": {
    "@herbsjs/herbarium": "^1.1.0",
    "@herbsjs/herbs": "^1.4.2",
    "@herbsjs/herbs2gql": "^2.0.1",
    "@herbsjs/herbs2knex": "^1.4.1",
    "@herbsjs/herbs2rest": "^2.0.1",
    "@herbsjs/herbsshelf": "^2.1.2",
    "apollo-server": "^3.6.1",
    "apollo-server-express": "^3.6.1",
    "cors": "^2.8.5",
    "deepmerge": "^4.2.2",
    "dotenv": "^12.0.3",
    "express": "^4.17.2",
    "graphql": "^15.8.0",
    "graphql-scalars": "^1.14.1",
    "graphql-tools": "^8.2.0",
    "lodash.camelcase": "^4.3.0",
    "mocha": "^9.1.4",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "sugar-env": "^1.5.14"
  },
  "devDependencies": {
    "prettier": "^2.5.1"
  }
}
